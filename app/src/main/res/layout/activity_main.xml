<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.udevel.widgetlab.MainActivity">

    <com.udevel.widgetlab.TypingIndicatorView
        android:id="@+id/typing_indicator_view01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="10dp"
        android:padding="8dp"
        app:animateFrequency="700"
        app:animationOrder="sequence"
        app:backgroundColor="#dfdfdf"
        app:backgroundType="rounded"
        app:dotAnimationDuration="600"
        app:dotAnimationType="Sliding"
        app:dotColor="#a4a4a4"
        app:dotCount="3"
        app:dotHorizontalSpacing="4dp"
        app:dotMaxCompressRatio="70%"
        app:dotSecondColor="#6ba4a4a4"
        app:dotSize="10dp"
        app:showBackground="true"/>

    <com.udevel.widgetlab.TypingIndicatorView
        android:id="@+id/typing_indicator_view02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="10dp"
        android:padding="8dp"
        app:animateFrequency="700"
        app:animationOrder="lastOneFirst"
        app:backgroundColor="#dfdfdf"
        app:backgroundType="rounded"
        app:dotAnimationDuration="600"
        app:dotAnimationType="Sliding"
        app:dotColor="#a4a4a4"
        app:dotCount="3"
        app:dotHorizontalSpacing="4dp"
        app:dotMaxCompressRatio="70%"
        app:dotSecondColor="#6ba4a4a4"
        app:dotSize="10dp"
        app:showBackground="true"/>

    <com.udevel.widgetlab.TypingIndicatorView
        android:id="@+id/typing_indicator_view03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="10dp"
        android:padding="8dp"
        app:animateFrequency="500"
        app:animationOrder="lastOneFirst"
        app:backgroundColor="#dfdfdf"
        app:backgroundType="rounded"
        app:dotAnimationDuration="800"
        app:dotAnimationType="Sliding"
        app:dotColor="#a4a4a4"
        app:dotCount="6"
        app:dotHorizontalSpacing="4dp"
        app:dotMaxCompressRatio="70%"
        app:dotSecondColor="#6ba4a4a4"
        app:dotSize="10dp"
        app:showBackground="true"/>

    <com.udevel.widgetlab.TypingIndicatorView
        android:id="@+id/typing_indicator_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="10dp"
        android:padding="20dp"
        app:backgroundColor="@color/colorPrimary"
        app:backgroundType="rounded"
        app:dotColor="@color/colorAccent"
        app:dotCount="4"
        app:showBackground="true"/>

    <com.udevel.widgetlab.TypingIndicatorView
        android:id="@+id/typing_indicator_view2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="10dp"
        app:dotCount="5"
        app:showBackground="false"/>

    <com.udevel.widgetlab.TypingIndicatorView
        android:id="@+id/typing_indicator_view3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="10dp"
        app:animateFrequency="3000"
        app:backgroundColor="@color/colorPrimary"
        app:backgroundType="square"
        app:dotAnimationDuration="3000"
        app:dotColor="#fcbbd1"
        app:dotCount="5"
        app:dotSecondColor="@color/colorPrimary"
        app:showBackground="true"/>

    <com.udevel.widgetlab.TypingIndicatorView
        android:id="@+id/typing_indicator_view4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="10dp"
        android:paddingBottom="8dp"
        android:paddingEnd="16dp"
        android:paddingStart="16dp"
        android:paddingTop="8dp"
        app:backgroundColor="#dfdfdf"
        app:backgroundType="rounded"
        app:dotColor="#a4a4a4"
        app:dotCount="3"
        app:dotSize="10dp"
        app:showBackground="true"/>

    <com.udevel.widgetlab.TypingIndicatorView
        android:id="@+id/typing_indicator_view5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="10dp"
        android:padding="8dp"
        app:backgroundColor="#dfdfdf"
        app:backgroundType="rounded"
        app:dotColor="#a4a4a4"
        app:dotCount="3"
        app:dotHorizontalSpacing="1dp"
        app:dotMaxCompressRatio="15%"
        app:dotSize="10dp"
        app:showBackground="true"/>

    <com.udevel.widgetlab.TypingIndicatorView
        android:id="@+id/typing_indicator_view7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="10dp"
        android:padding="4dp"
        app:animateFrequency="1500"
        app:animationOrder="sequence"
        app:backgroundColor="#dfdfdf"
        app:backgroundType="rounded"
        app:dotAnimationDuration="700"
        app:dotAnimationType="Wink"
        app:dotColor="#a4a4a4"
        app:dotCount="3"
        app:dotHorizontalSpacing="4dp"
        app:dotMaxCompressRatio="70%"
        app:dotSecondColor="#5e648e"
        app:dotSize="10dp"
        app:showBackground="true"/>

    <com.udevel.widgetlab.TypingIndicatorView
        android:id="@+id/typing_indicator_view8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="10dp"
        android:padding="4dp"
        app:animateFrequency="600"
        app:animationOrder="sequence"
        app:backgroundColor="#dfdfdf"
        app:backgroundType="rounded"
        app:dotColor="#dfdfdf"
        app:dotCount="3"
        app:dotHorizontalSpacing="4dp"
        app:dotMaxCompressRatio="70%"
        app:dotSecondColor="#a4a4a4"
        app:dotSize="10dp"
        app:showBackground="true"/>

    <com.udevel.widgetlab.TypingIndicatorView
        android:id="@+id/typing_indicator_view9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="10dp"
        android:paddingBottom="4dp"
        android:paddingEnd="16dp"
        android:paddingStart="16dp"
        android:paddingTop="4dp"
        app:animateFrequency="300"
        app:animationOrder="circular"
        app:backgroundColor="#dfdfdf"
        app:backgroundType="rounded"
        app:dotAnimationDuration="300"
        app:dotColor="#dfdfdf"
        app:dotCount="3"
        app:dotHorizontalSpacing="4dp"
        app:dotMaxCompressRatio="70%"
        app:dotSecondColor="#a4a4a4"
        app:dotSize="10dp"
        app:showBackground="true"/>

    <com.udevel.widgetlab.TypingIndicatorView
        android:id="@+id/typing_indicator_view10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="10dp"
        android:padding="8dp"
        app:animateFrequency="1000"
        app:animationOrder="sequence"
        app:backgroundColor="#dfdfdf"
        app:backgroundType="rounded"
        app:dotAnimationDuration="800"
        app:dotAnimationType="Disappear"
        app:dotColor="#a4a4a4"
        app:dotCount="3"
        app:dotHorizontalSpacing="4dp"
        app:dotMaxCompressRatio="70%"
        app:dotSecondColor="#dfdfdf"
        app:dotSize="16dp"
        app:showBackground="true"/>

    <com.udevel.widgetlab.TypingIndicatorView
        android:id="@+id/typing_indicator_view11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="10dp"
        android:padding="4dp"
        app:animateFrequency="500"
        app:animationOrder="sequence"
        app:backgroundColor="#dfdfdf"
        app:backgroundType="rounded"
        app:dotAnimationDuration="3000"
        app:dotColor="#dfdfdf"
        app:dotCount="6"
        app:dotHorizontalSpacing="4dp"
        app:dotMaxCompressRatio="70%"
        app:dotSecondColor="#a4a4a4"
        app:dotSize="20dp"
        app:showBackground="true"/>
</LinearLayout>
